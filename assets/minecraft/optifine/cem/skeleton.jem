					"left_arm_horse_jockey.visible": "varb.horse",
					"right_arm_normal.visible": "!varb.spider && !varb.horse",
					"right_arm_spider_jockey.visible": "varb.spider",
					"right_arm_horse_jockey.visible": "varb.horse",
					"left_leg_normal.visible": "!varb.spider && !varb.horse",
