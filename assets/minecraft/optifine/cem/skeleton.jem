			"animations": [
				{
					"var.jockey": "if(var.first_frame > 0, var.jockey, is_riding, 1, 0)",
					"varb.jockey": "var.jockey == 1",
					"var.time": "var.time + 1 * frame_time",
					"var.y": "if(var.time < 0.13, pos_y, frac(var.y))",
					"varb.spider": "varb.jockey && var.y > 0.8 && var.y < 0.9",
					"varb.horse": "varb.jockey && var.y > 0.35 && var.y < 0.45"
				},
				{
					"head_normal.visible": "!varb.spider && !varb.horse",
